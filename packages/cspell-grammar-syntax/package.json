{
  "name": "cspell-grammar-syntax",
  "version": "0.1.5",
  "description": "tmLanguage syntax files for use with cspell-grammar",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "tsc -p .",
    "prepublish": "npm run build",
    "coverage": "npm run generate-code-coverage",
    "generate-code-coverage": "NODE_ENV=test nyc --silent --no-clean --temp-dir=../../.nyc_output npm run test-ts",
    "test-ts": "NODE_ENV=test mocha --exit --require ts-node/register --recursive \"src/**/*.test.ts\"",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jason3S/cspell-grammar.git"
  },
  "keywords": [
    "cspell",
    "tmLanguage",
    "grammar",
    "syntax",
    "parse"
  ],
  "author": "Jason Dent",
  "license": "MIT",
  "files": [
    "dist/**",
    "syntax/**"
  ],
  "bugs": {
    "url": "https://github.com/Jason3S/cspell-grammar/issues"
  },
  "homepage": "https://github.com/Jason3S/cspell-grammar#readme",
  "dependencies": {
    "fs-extra": "^8.0.1"
  },
  "devDependencies": {},
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "src/**/*.test.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "clean": false,
    "cache": false,
    "temp-directory": "../../.nyc_output",
    "reporter": [
      "json",
      "html"
    ]
  }
}
